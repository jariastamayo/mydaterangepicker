{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": ".tmpbuild/dist",
    "rootDir": "./src/my-date-range-picker",
    "sourceMap": true,
    "removeComments": true,
    "moduleResolution": "node",
    "declaration": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "angularCompilerOptions": {
    "genDir": ".tmpbuild",
    "debug": false,
    "skipMetadataEmit": false
  },
  "include": [
    "src/my-date-range-picker/**/*"
  ]
}